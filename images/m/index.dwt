            //滚动务及返回顶部
            $(".gotop").hide();
            window.onscroll = function () {
                var rollBar_positon = document.documentElement.scrollTop || document.body.scrollTop;//获取滚动条位置，fixedHeight为当前窗口高度
                var isShow = false;
                var sear_hide_sta = $(".sear_hide").css("display");
                if (sear_hide_sta == "block") {
                    isShow = true;
                }
                else {
                    isShow = false;
                }
                if (rollBar_positon >= fixedHeight) {
                    $(".nxTop,.gotop").fadeIn("slow");
                    $(".fixedTop").hide();
                }
                /* if(isShow==true && rollBar_positon < fixedHeight){
                 $(".fixedTop").hide();
                 }*/
                else {
                    if (isShow == true) {
                        $(".fixedTop").hide();
                    }
                    else {
                        $(".fixedTop").show();
                        $(".nxTop,.gotop").fadeOut("slow");
                    }
                }
            }
        });
        function divShow() {
            $(".adShow").hide();
            $(".fixedTop,.pageBody,.gotop,footer").fadeIn("slow");
